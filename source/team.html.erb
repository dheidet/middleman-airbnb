<div class="text-center">
  <h1>Our team</h1>
  <% data.teams.each do |name, team| %>
    <div class="col-xs-12 col-sm-4">
        <%= image_tag team.picture, class:"avatar avatar-large" %>
      <div class="card-description">
        <h2><%= team.name %></h2>
      </div>
    </div>
  <% end %>
</div>
